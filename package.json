{
  "name": "karolo-bot",
  "version": "1.0.0",
  "description": "Bot and Web3 services for Karolo",
  "main": "index.js",
  "scripts": {
    "start:bot": "ts-node bot-service/index.ts",
    "start:web3": "ts-node web3-service/index.ts",
    "dev:bot": "nodemon --exec ts-node bot-service/index.ts",
    "dev:web3": "nodemon --exec ts-node web3-service/index.ts",
    "build": "tsc",
    "build:bot": "tsc bot-service/index.ts",
    "build:web3": "tsc web3-service/index.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "dv-sol-lib": "^1.3.19",
    "node-telegram-bot-api": "^0.64.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
